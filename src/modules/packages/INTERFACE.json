{
  "module": "packages",
  "description": "Intake, location, subNo unique lease, pickup/remove + events",
  "functions": [
    {
      "name": "intakeScan",
      "input": {
        "carrier": "string",
        "labelCode": "string",
        "customerName": "string?"
      },
      "output": {
        "packageId": "string",
        "status": "string"
      }
    },
    {
      "name": "assignLocation",
      "input": {
        "packageId": "string",
        "rackNo": "number",
        "positionNo": "number",
        "levelNo": "number"
      },
      "output": {
        "locationDisplay": "string"
      }
    },
    {
      "name": "assignSubNo",
      "input": {
        "packageId": "string",
        "subNo": "number"
      },
      "output": {
        "subNo": "number"
      }
    },
    {
      "name": "pickup",
      "input": {
        "packageId": "string"
      },
      "output": {
        "status": "PICKED_UP"
      }
    },
    {
      "name": "remove",
      "input": {
        "packageId": "string",
        "reason": "string"
      },
      "output": {
        "status": "REMOVED"
      }
    }
  ]
}